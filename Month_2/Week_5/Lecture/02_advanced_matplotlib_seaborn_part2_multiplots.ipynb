                           fontsize=12, fontweight='bold', color='green')\n\nelse:\n    # For sample data without proper monthly structure\n    ax_revenue.text(0.5, 0.5, 'Revenue Trend Analysis\\n(Requires time-series data)', \n                    ha='center', va='center', transform=ax_revenue.transAxes,\n                    fontsize=14, style='italic')\n\n# === SECTION 3: GEOGRAPHIC PERFORMANCE (Top right) ===\nax_geo = fig.add_subplot(gs[1, 5:])\n\n# Top performing states\ntop_states_perf = dashboard_data.groupby('customer_state').agg({\n    'order_value': 'sum',\n    'order_id': 'count',\n    'review_score': 'mean'\n}).reset_index().sort_values('order_value', ascending=True).tail(8)\n\n# Create horizontal bar chart\nbars = ax_geo.barh(range(len(top_states_perf)), top_states_perf['order_value'],\n                   color=corporate_colors[2], alpha=0.8)\nax_geo.set_yticks(range(len(top_states_perf)))\nax_geo.set_yticklabels(top_states_perf['customer_state'])\nax_geo.set_title('Revenue by State\\nTop Performing Markets', \n                fontsize=14, fontweight='bold')\nax_geo.set_xlabel('Total Revenue (R$)')\nax_geo.xaxis.set_major_formatter(plt.FuncFormatter(lambda x, p: f'R$ {x/1000:.0f}K'))\n\n# Highlight top performer\nbars[-1].set_color(corporate_colors[3])\nax_geo.annotate('Market Leader', \n                xy=(top_states_perf.iloc[-1]['order_value'], len(top_states_perf)-1),\n                xytext=(top_states_perf.iloc[-1]['order_value'] * 0.7, len(top_states_perf)-0.5),\n                arrowprops=dict(arrowstyle='->', color='red', lw=2),\n                fontsize=10, fontweight='bold', color='red')\n\n# === SECTION 4: CUSTOMER SATISFACTION MATRIX (Middle right) ===\nax_satisfaction = fig.add_subplot(gs[2, 5:])\n\n# Create satisfaction vs revenue scatter by category\ncategory_perf = dashboard_data.groupby('primary_category').agg({\n    'review_score': 'mean',\n    'order_value': 'sum',\n    'order_id': 'count'\n}).reset_index()\n\n# Bubble chart\nscatter = ax_satisfaction.scatter(category_perf['review_score'], \n                                 category_perf['order_value'],\n                                 s=category_perf['order_id']/20,  # Bubble size\n                                 alpha=0.7, c=range(len(category_perf)), \n                                 cmap='viridis', edgecolors='black', linewidth=1)\n\nax_satisfaction.set_title('Category Performance Matrix\\nSatisfaction vs Revenue', \n                         fontsize=14, fontweight='bold')\nax_satisfaction.set_xlabel('Average Review Score')\nax_satisfaction.set_ylabel('Total Revenue (R$)')\nax_satisfaction.yaxis.set_major_formatter(plt.FuncFormatter(lambda x, p: f'R$ {x/1000:.0f}K'))\nax_satisfaction.grid(True, alpha=0.3)\n\n# Add quadrant analysis lines\nsatisfaction_median = category_perf['review_score'].median()\nrevenue_median = category_perf['order_value'].median()\nax_satisfaction.axvline(satisfaction_median, color='red', linestyle='--', alpha=0.5)\nax_satisfaction.axhline(revenue_median, color='red', linestyle='--', alpha=0.5)\n\n# Add quadrant labels\nax_satisfaction.text(satisfaction_median + 0.1, revenue_median * 1.5, \n                    'High Satisfaction\\nHigh Revenue', fontsize=9, \n                    bbox=dict(boxstyle='round', facecolor='lightgreen', alpha=0.7))\n\n# === SECTION 5: OPERATIONAL METRICS (Bottom section) ===\n\n# Payment Methods Analysis (Bottom left)\nax_payment = fig.add_subplot(gs[3:5, :3])\n\npayment_analysis = dashboard_data.groupby('payment_type').agg({\n    'order_id': 'count',\n    'order_value': ['sum', 'mean']\n}).reset_index()\npayment_analysis.columns = ['payment_type', 'order_count', 'total_revenue', 'avg_order_value']\n\n# Create stacked analysis\nx_pos = np.arange(len(payment_analysis))\nwidth = 0.4\n\n# Normalize for percentage view\npayment_analysis['revenue_pct'] = (payment_analysis['total_revenue'] / \n                                  payment_analysis['total_revenue'].sum() * 100)\npayment_analysis['count_pct'] = (payment_analysis['order_count'] / \n                                payment_analysis['order_count'].sum() * 100)\n\nbars1 = ax_payment.bar(x_pos - width/2, payment_analysis['revenue_pct'], \n                      width, label='Revenue %', color=corporate_colors[4], alpha=0.8)\nbars2 = ax_payment.bar(x_pos + width/2, payment_analysis['count_pct'], \n                      width, label='Order Count %', color=corporate_colors[5], alpha=0.8)\n\nax_payment.set_title('Payment Method Distribution\\nRevenue vs Transaction Volume', \n                    fontsize=14, fontweight='bold')\nax_payment.set_xlabel('Payment Method')\nax_payment.set_ylabel('Percentage (%)')\nax_payment.set_xticks(x_pos)\nax_payment.set_xticklabels(payment_analysis['payment_type'], rotation=45, ha='right')\nax_payment.legend()\nax_payment.grid(True, alpha=0.3)\n\n# Order Value Distribution (Bottom center)\nax_value_dist = fig.add_subplot(gs[3:5, 3:5])\n\n# Create value tier analysis\nvalue_tier_analysis = dashboard_data.groupby('value_tier').agg({\n    'order_id': 'count',\n    'order_value': 'sum',\n    'review_score': 'mean'\n}).reset_index()\n\n# Donut chart for value distribution\nwedges, texts, autotexts = ax_value_dist.pie(value_tier_analysis['order_id'], \n                                            labels=value_tier_analysis['value_tier'],\n                                            autopct='%1.1f%%',\n                                            startangle=90,\n                                            colors=corporate_colors[:len(value_tier_analysis)],\n                                            wedgeprops=dict(width=0.5))  # Donut style\n\nax_value_dist.set_title('Customer Value Distribution\\nOrder Volume by Tier', \n                       fontsize=14, fontweight='bold')\n\n# Performance Summary Table (Bottom right)\nax_summary = fig.add_subplot(gs[3:5, 5:])\nax_summary.axis('off')\n\n# Create performance summary table\nsummary_data = [\n    ['Metric', 'Value', 'Trend'],\n    ['Total Customers', f\"{dashboard_data['customer_id'].nunique():,}\", 'â†—'],\n    ['Avg Items/Order', f\"{dashboard_data['item_count'].mean():.1f}\", 'â†’'],\n    ['Freight Ratio', f\"{dashboard_data['freight_ratio'].mean():.1%}\", 'â†˜'],\n    ['5-Star Reviews', f\"{(dashboard_data['review_score'] == 5).mean():.1%}\", 'â†—'],\n    ['Premium Orders', f\"{(dashboard_data['value_tier'] == 'Premium').mean():.1%}\", 'â†—']\n]\n\n# Create table\ntable = ax_summary.table(cellText=summary_data[1:], \n                        colLabels=summary_data[0],\n                        cellLoc='center',\n                        loc='center',\n                        bbox=[0, 0, 1, 1])\n\ntable.auto_set_font_size(False)\ntable.set_fontsize(10)\ntable.scale(1, 2)\n\n# Style the table\nfor i in range(len(summary_data)):\n    for j in range(len(summary_data[0])):\n        cell = table[(i, j)]\n        if i == 0:  # Header row\n            cell.set_facecolor(corporate_colors[0])\n            cell.set_text_props(weight='bold', color='white')\n        else:\n            cell.set_facecolor('lightgray' if i % 2 == 0 else 'white')\n            if j == 2:  # Trend column\n                color = 'green' if cell.get_text().get_text() == 'â†—' else 'red' if cell.get_text().get_text() == 'â†˜' else 'blue'\n                cell.set_text_props(color=color, weight='bold', size=14)\n\nax_summary.set_title('Performance Summary\\nKey Operational Metrics', \n                    fontsize=14, fontweight='bold', y=0.95)\n\nplt.show()\n\nprint(\"ðŸ¢ Executive Dashboard Features:\")\nprint(\"â€¢ Comprehensive KPI section for quick overview\")\nprint(\"â€¢ Multi-metric trend analysis with growth indicators\")\nprint(\"â€¢ Geographic performance with market leadership identification\")\nprint(\"â€¢ Customer satisfaction matrix for strategic positioning\")\nprint(\"â€¢ Operational metrics with trend indicators\")\nprint(\"â€¢ Professional styling suitable for board presentations\")\nprint(\"â€¢ Color-coded performance indicators\")\nprint(\"â€¢ Interactive elements through annotations and highlights\")"
